{
    "name": "ngif",
    "version": "2.3.45-beta",
    "description": "A fast and simple way to view gifs!",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder"
    },
    "license": "MIT",
    "author": "Zell Apps",
    "devDependencies": {
        "electron": "^16.0.2",
        "electron-builder": "^22.14.5",
        "electron-packager": "^15.4.0"
    },
    "build": {
        "appId": "com.zellapps.ngif",
        "win": {
            "target": "nsis",
            "icon": "build/icon.ico"
        },
        "darwin": {
            "target": "default",
            "icon": "build/icon.ico"
        },
        "linux": {
            "target": "deb",
            "icon": "build/icon.ico"
        }
    }
}
